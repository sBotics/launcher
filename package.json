{
    "name": "sBotics",
    "version": "2.4.0",
    "description": "sBotics",
    "main": "main.js",
    "author": "Natalnet Labs <sboticssimulator@gmail.com>",
    "license": "(ISC OR GPL-3.0)",
    "scripts": {
        "start": "electron .",
        "build-win": "electron-builder build --win --ia32 --publish never",
        "deploy-win": "electron-builder build --win --ia32 --publish always",
        "build-mac": "electron-builder build --mac --publish never",
        "deploy-mac": "electron-builder build --mac --publish onTagOrDraft",
        "build-linux": "electron-builder build --linux --publish never",
        "deploy-linux": "electron-builder build --linux --x64 --publish onTagOrDraft",
        "build-linux-deb": "electron-builder build --linux deb --publish never ",
        "deploy-linux-deb": "electron-builder build --linux deb --publish onTagOrDraft"
    },
    "build": {
        "asar": false,
        "productName": "sBotics",
        "win": {
            "icon": "./icons/icon.ico"
        },
        "linux": {
            "target": "AppImage"
        },
        "mac": {
            "icon": "./icons/icon.icns",
            "target": "default"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sBotics/launcher.git"
    },
    "dependencies": {
        "@xmcl/unzip": "^1.2.0",
        "axios": "^0.21.1",
        "chmodr": "^1.2.0",
        "electron-updater": "^4.3.8",
        "fs-extra": "^9.1.0",
        "jQuery": "^1.7.4",
        "luxon": "^1.24.1",
        "mkdirp": "^1.0.4",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "electron": "^9.0.5",
        "electron-builder": "^22.7.0",
        "electron-packager": "^15.0.0",
        "electron-reloader": "^1.2.0",
        "prettier": "2.2.1"
    }
}